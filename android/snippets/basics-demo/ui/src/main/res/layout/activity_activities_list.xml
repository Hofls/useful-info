<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.activities.ActivitiesList">


    <LinearLayout
        android:layout_width="226dp"
        android:layout_height="696dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:orientation="vertical"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <Button
            android:id="@+id/button24"
            android:layout_width="91dp"
            android:layout_height="45dp"
            android:onClick="sendDataToActivity"
            android:text="@string/button_send" />

        <Button
            android:id="@+id/button16"
            android:layout_width="wrap_content"
            android:layout_height="52dp"
            android:onClick="toLifecycle"
            android:text="Lifecycle" />

        <Button
            android:id="@+id/button19"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:onClick="toSettings"
            android:text="Settings" />

        <Button
            android:id="@+id/button17"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:onClick="toCompose"
            android:text="Compose" />

        <Button
            android:id="@+id/button25"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:onClick="toAnotherApp1"
            android:text="Another app 1" />

        <Button
            android:id="@+id/button26"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:onClick="toAnotherApp2"
            android:text="Another app 2" />
    </LinearLayout>


</androidx.constraintlayout.widget.ConstraintLayout>